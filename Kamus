meme_dict = {
    "CRINGE": "Sesuatu yang sangat aneh atau memalukan",
    "LOL": "Tanggapan umum terhadap sesuatu yang lucu",
    "ROFL": "Tanggapan terhadap lelucon yang sangat lucu",
    "SHEESH": "Ekspresi ketidaksetujuan ringan atau keheranan",
    "CREEPY": "Menakutkan atau membuat tidak nyaman",
    "AGGRO": "Menjadi agresif atau marah"
}

for i in range(5):
    word = input("Ketik kata yang tidak kamu mengerti (gunakan huruf kapital semua!): ")

    if word in meme_dict.keys():
        print({meme_dict[word]})
    else:
        print("Tidak ada di dalam kamus.")
